	year = {2011},
	keywords = {Baer-Nunziato model, Discontinuous Galerkin method, Finite volume schemes, Multi-phase flows, Osher Riemann solver, P NP Mschemes, Path-conservative schemes, Pitman \& Le model, Shallow water equations, Well-balanced schemes, P NP},
