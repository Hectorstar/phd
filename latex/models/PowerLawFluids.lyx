\begin_layout Standard
Take the singular value decomposition 
\begin_inset Formula $A=U\Sigma V^{T}$
\end_inset

.
 Note that:
\end_layout

\begin_layout Standard
\begin_inset Formula 
\begin{equation}
\sigma=-\rho c_{s}^{2}A^{T}A\dev\left(A^{T}A\right)=-\rho c_{s}^{2}V\Sigma^{2}\dev\left(\Sigma^{2}\right)V^{T}
\end{equation}

\end_inset


\end_layout

\begin_layout Standard
Thus:
\end_layout

\begin_layout Standard
\begin_inset Formula 
\begin{equation}
\left\Vert \sigma\right\Vert _{F}^{n}=\rho^{n}c_{s}^{2n}\left\Vert \Sigma^{2}\dev\left(\Sigma^{2}\right)\right\Vert _{F}^{n}
\end{equation}

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\begin{equation}
\frac{d\Sigma}{dt}=-\frac{3}{\tau_{0}}\left(\frac{\rho}{\rho_{0}}\right)^{\frac{5}{3}}\rho^{n}c_{s}^{2n}\left\Vert \Sigma^{2}\dev\left(\Sigma^{2}\right)\right\Vert _{F}^{n}\Sigma\dev\left(\Sigma^{2}\right)
\end{equation}

\end_inset


\end_layout

\begin_layout Standard
Letting 
\begin_inset Formula $x_{i}=\frac{a_{i}^{2}}{\det\left(A\right)^{\frac{2}{3}}}=\frac{a_{i}^{2}}{\left(\frac{\rho}{\rho_{0}}\right)^{\frac{2}{3}}}$
\end_inset

 then 
\begin_inset Formula $\Sigma^{2}=\det\left(A\right)^{\frac{2}{3}}X$
\end_inset

 where 
\begin_inset Formula $X=\diag\left(x_{1},x_{2},x_{3}\right)$
\end_inset

.
 Thus, we have:
\end_layout

\begin_layout Standard
\begin_inset Formula 
\begin{equation}
\frac{dx_{i}}{d\tilde{t}}=-3\left\Vert X\dev\left(X\right)\right\Vert _{F}^{n}x_{i}\left(x_{i}-\bar{x}\right)\label{eq:StretchODESystem-2}
\end{equation}

\end_inset


\end_layout

\begin_layout Standard
where:
\end_layout

\begin_layout Standard
\begin_inset Formula 
\begin{equation}
\tilde{t}=\frac{2}{\tau_{0}}\left(\frac{\rho}{\rho_{0}}\right)^{\frac{4n+7}{3}}\left(\rho c_{s}^{2}\right)^{n}t
\end{equation}

\end_inset


\end_layout

\begin_layout Standard
Note that:
