\begin_layout Standard
\begin_inset Formula 
\begin{align}
o_{j} & =\sum_{\left|\alpha\right|=1}^{N}\int_{C_{i}}\left(D^{\alpha}w_{j}\right)^{2}d\boldsymbol{x}\\
 & =\int_{0}^{1}\left\{ \left(\frac{\partial w_{j}}{\partial x}\right)^{2}+\left(\frac{\partial w_{j}}{\partial y}\right)^{2}+\left(\frac{\partial^{2}w_{j}}{\partial x^{2}}\right)^{2}+\left(\frac{\partial^{2}w_{j}}{\partial y^{2}}\right)^{2}+\left(\frac{\partial^{2}w_{j}}{\partial x\partial y}\right)^{2}\right\} d\boldsymbol{\chi}\nonumber \\
 & =\int_{0}^{1}\left\{ \left(\tilde{w}_{pq}\psi_{p}^{'}\left(x\right)\psi_{q}\left(y\right)\right)^{2}+\left(\tilde{w}_{pq}\psi_{p}\left(x\right)\psi_{q}^{'}\left(y\right)\right)^{2}+\left(\tilde{w}_{pq}\psi_{p}^{''}\left(x\right)\psi_{q}\left(y\right)\right)^{2}+\left(\tilde{w}_{pq}\psi_{p}\left(x\right)\psi_{q}^{'}\left(y\right)\right)^{2}+\left(\tilde{w}_{pq}\psi_{p}^{'}\left(x\right)\psi_{q}^{'}\left(y\right)\right)^{2}\right\} d\boldsymbol{\chi}\nonumber \\
 & =\int_{0}^{1}\left\{ \tilde{w}_{p_{1}q_{1}}\psi_{p_{1}}^{'}\left(x\right)\psi_{q_{1}}\left(y\right)\tilde{w}_{p_{2}q_{2}}\psi_{p_{2}}^{'}\left(x\right)\psi_{q_{2}}\left(y\right)+\cdots\right\} d\boldsymbol{\chi}\nonumber \\
 & =\tilde{w}_{p_{1}q_{1}}\tilde{w}_{p_{2}q_{2}}\int_{C_{i}}\left\{ \psi_{p_{1}}^{'}\left(x\right)\psi_{q_{1}}\left(y\right)\psi_{p_{2}}^{'}\left(x\right)\psi_{q_{2}}\left(y\right)+\psi_{p_{1}}\left(x\right)\psi_{q_{1}}\left(y\right)\psi_{p_{2}}\left(x\right)\psi_{q_{2}}\left(y\right)+\cdots\right\} d\boldsymbol{\chi}\nonumber \\
 & =\tilde{w}_{p_{1}q_{1}}\tilde{w}_{p_{2}q_{2}}\Sigma_{p_{1}p_{2}q_{1}q_{2}}\nonumber 
\end{align}

\end_inset


