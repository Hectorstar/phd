\begin_layout Section*
Spatial Derivatives
\end_layout

\begin_layout Standard
Note that:
\end_layout

\begin_layout Standard
\begin_inset Formula 
\begin{align}
\frac{\partial T}{\partial x_{k}} & =\frac{1}{c_{v}\left(\gamma-1\right)\rho}\frac{\partial p}{\partial x_{k}}-\frac{p+p_{\infty}}{c_{v}\left(\gamma-1\right)\rho^{2}}\frac{\partial\rho}{\partial x_{k}}\label{eq:dTdx}\\
 & =\frac{T}{p+p_{\infty}}\frac{\partial p}{\partial x_{k}}-\frac{T}{\rho}\frac{\partial\rho}{\partial x_{k}}\nonumber 
\end{align}

\end_inset


\end_layout

\begin_layout Standard
Thus:
\end_layout

\begin_layout Standard
\begin_inset Formula 
\begin{align}
\frac{\partial q_{i}}{\partial x_{k}} & =\alpha^{2}\frac{\partial T}{\partial x_{k}}J_{i}+\alpha^{2}T\frac{\partial J_{i}}{\partial x_{k}}\label{eq:dqdx}\\
 & =\frac{\alpha^{2}TJ_{i}}{p+p_{\infty}}\frac{\partial p}{\partial x_{k}}-\frac{\alpha^{2}TJ_{i}}{\rho}\frac{\partial\rho}{\partial x_{k}}+\alpha^{2}T\frac{\partial J_{i}}{\partial x_{k}}\nonumber \\
 & =\frac{q_{i}}{p+p_{\infty}}\frac{\partial p}{\partial x_{k}}-\frac{q_{i}}{\rho}\frac{\partial\rho}{\partial x_{k}}+\alpha^{2}T\frac{\partial J_{i}}{\partial x_{k}}\nonumber 
\end{align}

\end_inset


\end_layout

\begin_layout Standard
Additionally, with 
\begin_inset Formula $E$
\end_inset

 defined as in 
\begin_inset CommandInset ref
LatexCommand eqref
reference "eq:EnergyDefinition"

\end_inset

:
\end_layout

\begin_layout Standard
\begin_inset Formula 
\begin{equation}
\frac{\partial E}{\partial x_{k}}=\frac{1}{\left(\gamma-1\right)\rho}\frac{\partial p}{\partial x_{i}}-\frac{E_{1}}{\rho}\frac{\partial\rho}{\partial x_{k}}+\psi_{mn}\frac{\partial A_{mn}}{\partial x_{k}}+\alpha^{2}J_{m}\frac{\partial J_{m}}{\partial x_{k}}+v_{m}\frac{\partial v_{m}}{\partial x_{k}}\label{eq:dEdx}
\end{equation}

\end_inset


\end_layout

\begin_layout Standard
As 
\begin_inset Formula $\frac{\partial E}{\partial A}$
\end_inset

 is a function of 
\begin_inset Formula $A$
\end_inset

 only, 
\begin_inset Formula $\frac{\partial\sigma}{\partial\rho}=\frac{\sigma}{\rho}$
\end_inset

, giving:
\end_layout

\begin_layout Standard
\begin_inset Formula 
\begin{align}
\frac{\partial\sigma_{ik}}{\partial x_{k}} & =\frac{\partial\sigma_{ik}}{\partial\rho}\frac{\partial\rho}{\partial x_{k}}+\frac{\partial\sigma_{ik}}{\partial A_{mn}}\frac{\partial A_{mn}}{\partial x_{k}}\label{eq:dsigma/dx}\\
 & =\frac{\sigma_{ik}}{\rho}\frac{\partial\rho}{\partial x_{k}}+\frac{\partial\sigma_{ik}}{\partial A_{mn}}\frac{\partial A_{mn}}{\partial x_{k}}\nonumber 
\end{align}

\end_inset


\end_layout

\begin_layout Section*
Derivatives with Respect to the Distortion Tensor
\end_layout

\begin_layout Standard
Note that:
\end_layout

\begin_layout Standard
\begin_inset Formula 
\begin{equation}
\frac{\partial A_{ki}A_{kj}}{\partial A_{mn}}=\frac{\partial A_{ki}}{\partial A_{mn}}A_{kj}+A_{ki}\frac{\partial A_{kj}}{\partial A_{mn}}=\delta_{km}\delta_{in}A_{kj}+A_{ki}\delta_{km}\delta_{jn}
\end{equation}

\end_inset


\end_layout

\begin_layout Standard
As 
\begin_inset Formula $G_{ij}=\left(A^{T}A\right)_{j}=A_{ki}A_{kj}$
\end_inset

:
\end_layout

\begin_layout Standard
\begin_inset ERT
status open

\begin_layout Plain Layout


