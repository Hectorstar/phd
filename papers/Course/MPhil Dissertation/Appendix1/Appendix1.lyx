end{subequations}
\end_layout

\end_inset


\end_layout

\begin_layout Standard
Combining these results:
\end_layout

\begin_layout Standard
\begin_inset Formula 
\begin{equation}
\frac{\partial\dev\left(G\right)_{ij}}{\partial A_{mn}}=\delta_{in}A_{mj}+\delta_{jn}A_{mi}-\frac{2}{3}\delta_{ij}A_{mn}
\end{equation}

\end_inset


\end_layout

\begin_layout Standard
Thus:
\end_layout

\begin_layout Standard

\size small
\begin_inset Formula 
\begin{align}
\frac{\partial\left(G\dev\left(G\right)\right)_{ij}}{\partial A_{mn}} & =\frac{\partial G_{it}}{\partial A_{mn}}\dev\left(G\right)_{tj}+G_{it}\frac{\partial\dev\left(G\right)_{tj}}{\partial A_{mn}}\\
 & =\left(\delta_{in}A_{mt}+\delta_{tn}A_{mi}\right)\left(A_{kt}A_{kj}-\frac{1}{3}A_{kl}A_{kl}\delta_{tj}\right)\nonumber \\
 & \quad+\left(A_{ki}A_{kt}\right)\left(\delta_{tn}A_{mj}+\delta_{jn}A_{mt}-\frac{2}{3}\delta_{tj}A_{mn}\right)\nonumber \\
 & =\delta_{in}\left(A_{mt}A_{kt}A_{kj}-\frac{1}{3}A_{mj}A_{kl}A_{kl}\right)+\delta_{jn}\left(A_{ki}A_{kt}A_{mt}-\frac{1}{3}A_{mi}A_{kl}A_{kl}\right)\nonumber \\
 & \quad+A_{mi}A_{kn}A_{kj}+A_{ki}A_{kn}A_{mj}-\frac{2}{3}A_{ki}A_{kj}A_{mn}\nonumber 
\end{align}

\end_inset


\end_layout

\begin_layout Standard
Thus, as 
\begin_inset Formula $\sigma=-\rho c_{s}^{2}G\dev\left(G\right)$
\end_inset

, the partial derivative of 
\begin_inset Formula $\sigma$
\end_inset

 with respect to 
\begin_inset Formula $A$
\end_inset

, holding 
\begin_inset Formula $\rho$
\end_inset

 constant, is given by:
\end_layout

\begin_layout Standard
\begin_inset Formula 
\begin{equation}
\frac{\partial\sigma_{ij}}{\partial A_{mn}}=-c_{s}^{2}\rho\left(\delta_{in}\left(A\dev\left(G\right)\right)_{mj}+\delta_{jn}\left(A\dev\left(G\right)\right)_{mi}+A_{mi}G_{jn}+A_{mj}G_{in}-\frac{2}{3}G_{ij}A_{mn}\right)\label{eq:dsigma/dA}
\end{equation}

\end_inset


\end_layout

