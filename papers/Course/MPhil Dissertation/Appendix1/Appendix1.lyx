begin{subequations}
\end_layout

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\begin{align}
\frac{\partial G_{ij}}{\partial A_{mn}} & =\delta_{in}A_{mj}+\delta_{jn}A_{mi}\\
\frac{\partial\tr\left(G\right)}{\partial A_{mn}}=\frac{\partial G_{kk}}{\partial A_{mn}} & =2A_{mn}
\end{align}

\end_inset


\end_layout

\begin_layout Standard
\begin_inset ERT
status open

\begin_layout Plain Layout


