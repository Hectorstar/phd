\end_layout

\begin_layout Standard
where 
\begin_inset Formula $\frac{DX}{Dt}=\frac{\partial X}{\partial t}+v_{k}\frac{\partial X}{\partial x_{k}}$
\end_inset

 is the material derivative of quantity 
\begin_inset Formula $X$
\end_inset

.
 Inserting the first, third, and fourth equations into the second:
\end_layout

\begin_layout Standard

\size small
\begin_inset Formula 
\begin{align}
0 & =\frac{\partial\left(\rho E\right)}{\partial\rho}\left(-\rho\frac{\partial v_{k}}{\partial x_{k}}\right)+\frac{\partial\left(\rho E\right)}{\partial v_{i}}\left(-\frac{1}{\rho}\frac{\partial\left(p\delta_{ik}-\sigma_{ik}\right)}{\partial x_{k}}\right)\\
 & +\frac{\partial\left(\rho E\right)}{\partial A_{ij}}\left(-\frac{\psi_{ij}}{\theta_{1}(\tau_{1})}-A_{ik}\frac{\partial v_{k}}{\partial x_{j}}\right)+\frac{\partial\left(\rho E\right)}{\partial J_{i}}\left(-\frac{H_{i}}{\theta_{2}(\tau_{2})}-\frac{1}{\rho}\frac{\partial T}{\partial x_{i}}\right)\nonumber \\
 & +\frac{\partial\left(\rho E\right)}{\partial p}\left(\frac{\partial p}{\partial t}+v_{k}\frac{\partial p}{\partial x_{k}}\right)+\rho E\frac{\partial v_{k}}{\partial x_{k}}+\frac{\partial\left(v_{i}\left(p\delta_{ik}-\sigma_{ik}\right)+q_{k}\right)}{\partial x_{k}}\nonumber 
\end{align}

\end_inset


\end_layout

\begin_layout Standard
When holding 
\begin_inset Formula $p$
\end_inset

 constant rather than 
\begin_inset Formula $s$
\end_inset

, for the equations of state used in this study:
\end_layout

\begin_layout Standard
\begin_inset Formula 
\begin{equation}
\rho^{2}\frac{\partial E}{\partial\rho}=-\frac{p}{\gamma-1}
\end{equation}

\end_inset


\end_layout

\begin_layout Standard
Thus, applying the product rule to 
\begin_inset Formula $\frac{\partial\left(\rho E\right)}{\partial\rho}$
\end_inset

:
\end_layout

\begin_layout Standard

\size small
\begin_inset Formula 
\begin{align}
0= & \frac{p}{\gamma-1}\frac{\partial v_{k}}{\partial x_{k}}-\frac{\partial\left(\rho E\right)}{\partial v_{i}}\left(\frac{1}{\rho}\frac{\partial\left(p\delta_{ik}-\sigma_{ik}\right)}{\partial x_{k}}\right)\\
 & -\frac{\partial\left(\rho E\right)}{\partial A_{ij}}\left(\frac{\psi_{ij}}{\theta_{1}(\tau_{1})}+A_{ik}\frac{\partial v_{k}}{\partial x_{j}}\right)-\frac{\partial\left(\rho E\right)}{\partial J_{i}}\left(\frac{H_{i}}{\theta_{2}(\tau_{2})}+\frac{1}{\rho}\frac{\partial T}{\partial x_{i}}\right)\nonumber \\
 & +\frac{\partial\left(\rho E\right)}{\partial p}\left(\frac{\partial p}{\partial t}+v_{k}\frac{\partial p}{\partial x_{k}}\right)+\frac{\partial\left(v_{i}\left(p\delta_{ik}-\sigma_{ik}\right)+q_{k}\right)}{\partial x_{k}}\nonumber 
\end{align}

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula $\frac{\partial\left(\rho E\right)}{\partial v_{i}}=\rho v_{i}$
\end_inset

, so applying the product rule to the last term:
\end_layout

\begin_layout Standard

\size small
\begin_inset Formula 
\begin{align}
0= & \frac{p}{\gamma-1}\frac{\partial v_{k}}{\partial x_{k}}-\frac{\partial\left(\rho E\right)}{\partial A_{ij}}\left(\frac{\psi_{ij}}{\theta_{1}(\tau_{1})}+A_{ik}\frac{\partial v_{k}}{\partial x_{j}}\right)-\frac{\partial\left(\rho E\right)}{\partial J_{i}}\left(\frac{H_{i}}{\theta_{2}(\tau_{2})}+\frac{1}{\rho}\frac{\partial T}{\partial x_{i}}\right)\\
 & +\frac{\partial\left(\rho E\right)}{\partial p}\left(\frac{\partial p}{\partial t}+v_{k}\frac{\partial p}{\partial x_{k}}\right)+\left(p\delta_{ik}-\sigma_{ik}\right)\frac{\partial v_{i}}{\partial x_{k}}+\frac{\partial q_{k}}{\partial x_{k}}\nonumber 
\end{align}

\end_inset


\end_layout

\begin_layout Standard
Thus:
\end_layout

\begin_layout Standard

\size small
\begin_inset Formula 
\begin{align}
\frac{\psi_{ij}}{\theta_{1}(\tau_{1})}\frac{\partial\left(\rho E\right)}{\partial A_{ij}}+\frac{H_{i}}{\theta_{2}(\tau_{2})}\frac{\partial\left(\rho E\right)}{\partial J_{i}}= & -\frac{\partial\left(\rho E\right)}{\partial A_{ij}}A_{ik}\frac{\partial v_{k}}{\partial x_{j}}-\frac{\partial\left(\rho E\right)}{\partial J_{i}}\frac{1}{\rho}\frac{\partial T}{\partial x_{i}}\\
 & +\frac{\partial\left(\rho E\right)}{\partial p}\left(\frac{\partial p}{\partial t}+v_{k}\frac{\partial p}{\partial x_{k}}\right)+\frac{\gamma p}{\gamma-1}\frac{\partial v_{k}}{\partial x_{k}}\nonumber \\
 & -\sigma_{ik}\frac{\partial v_{i}}{\partial x_{k}}+\frac{\partial q_{k}}{\partial x_{k}}\nonumber 
\end{align}

\end_inset


\end_layout

\begin_layout Standard
Using 
\begin_inset CommandInset ref
LatexCommand eqref
reference "eq:dTdx"

\end_inset

, 
\begin_inset CommandInset ref
LatexCommand eqref
reference "eq:dqdx"

\end_inset

 and the fact that 
\begin_inset Formula $\frac{\partial\left(\rho E\right)}{\partial J_{i}}=\rho\alpha^{2}J_{i}$
\end_inset

:
\end_layout

\begin_layout Standard

\size small
\begin_inset Formula 
\begin{align}
\frac{\psi_{ij}}{\theta_{1}(\tau_{1})}\frac{\partial\left(\rho E\right)}{\partial A_{ij}}+\frac{H_{i}}{\theta_{2}(\tau_{2})}\frac{\partial\left(\rho E\right)}{\partial J_{i}}= & -\frac{\partial\left(\rho E\right)}{\partial A_{ij}}A_{ik}\frac{\partial v_{k}}{\partial x_{j}}+\frac{\partial\left(\rho E\right)}{\partial p}\left(\frac{\partial p}{\partial t}+v_{k}\frac{\partial p}{\partial x_{k}}\right)\\
 & +\frac{\gamma p}{\gamma-1}\frac{\partial v_{k}}{\partial x_{k}}-\sigma_{ik}\frac{\partial v_{i}}{\partial x_{k}}+\alpha^{2}T\frac{\partial J_{k}}{\partial x_{k}}\nonumber 
\end{align}

\end_inset


\end_layout

\begin_layout Standard
Reindexing the first term gives:
\end_layout

\begin_layout Standard

\size small
\begin_inset Formula 
\begin{align}
\frac{\psi_{ij}}{\theta_{1}(\tau_{1})}\frac{\partial\left(\rho E\right)}{\partial A_{ij}}+\frac{H_{i}}{\theta_{2}(\tau_{2})}\frac{\partial\left(\rho E\right)}{\partial J_{i}}= & \frac{\partial\left(\rho E\right)}{\partial p}\left(\frac{\partial p}{\partial t}+v_{k}\frac{\partial p}{\partial x_{k}}\right)-\left(\frac{\partial\left(\rho E\right)}{\partial A_{jk}}A_{ji}+\sigma_{ik}\right)\frac{\partial v_{i}}{\partial x_{k}}\\
 & +\frac{\gamma p}{\gamma-1}\frac{\partial v_{k}}{\partial x_{k}}+\alpha^{2}T\frac{\partial J_{k}}{\partial x_{k}}\nonumber 
\end{align}

\end_inset


\end_layout

\begin_layout Standard
As 
\begin_inset Formula $\sigma=-\rho A^{T}\frac{\partial E}{\partial A}$
\end_inset

, the second term disappears.
 Thus, noting that 
\begin_inset Formula $\frac{\partial\left(\rho E\right)}{\partial p}=\frac{1}{\gamma-1}$
\end_inset

, 
\begin_inset Formula $\psi=\frac{\partial E}{\partial A}$
\end_inset

, and 
\begin_inset Formula $H=\frac{\partial E}{\partial J}$
\end_inset

, the full system is:
\end_layout

