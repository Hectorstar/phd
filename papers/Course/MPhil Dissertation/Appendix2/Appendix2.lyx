\begin_layout Standard

\size small
\begin_inset Formula 
\begin{align}
\frac{\partial\rho}{\partial t}+v_{k}\frac{\partial\rho}{\partial x_{k}}+\rho\frac{\partial v_{k}}{\partial x_{k}} & =0\\
\frac{\partial p}{\partial t}+v_{k}\frac{\partial p}{\partial x_{k}}+\gamma p\frac{\partial v_{k}}{\partial x_{k}}+\left(\gamma-1\right)\alpha^{2}T\frac{\partial J_{k}}{\partial x_{k}} & =\left(\gamma-1\right)\rho\left(\frac{\psi_{ij}\psi_{ij}}{\theta_{1}(\tau_{1})}+\frac{H_{i}H_{i}}{\theta_{2}(\tau_{2})}\right)\\
\frac{\partial A_{ij}}{\partial t}+v_{k}\frac{\partial A_{ij}}{\partial x_{k}}+A_{ik}\frac{\partial v_{k}}{\partial x_{j}} & =-\frac{\psi_{ij}}{\theta_{1}(\tau_{1})}\\
\frac{\partial v_{i}}{\partial t}+v_{k}\frac{\partial v_{i}}{\partial x_{k}}+\frac{1}{\rho}\frac{\partial p}{\partial x_{i}}-\frac{\sigma_{ik}}{\rho^{2}}\frac{\partial\rho}{\partial x_{k}}-\frac{1}{\rho}\frac{\partial\sigma_{ik}}{\partial A_{mn}}\frac{\partial A_{mn}}{\partial x_{k}} & =0\\
\frac{\partial J_{i}}{\partial t}+v_{k}\frac{\partial J_{i}}{\partial x_{k}}+\frac{T}{\rho\left(p+p_{\infty}\right)}\frac{\partial p}{\partial x_{i}}-\frac{T}{\rho^{2}}\frac{\partial\rho}{\partial x_{i}} & =-\frac{H_{i}}{\theta_{2}(\tau_{2})}
\end{align}

