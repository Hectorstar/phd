\begin_layout Standard

\size small
\begin_inset Formula 
\begin{align}
\frac{\partial\rho}{\partial t}+\frac{\partial\left(\rho v_{k}\right)}{\partial x_{k}} & =0\\
\frac{\partial\left(\rho E\right)}{\partial t}+v_{k}\frac{\partial\left(\rho E\right)}{\partial x_{k}}+\rho E\frac{\partial v_{k}}{\partial x_{k}}+\frac{\partial\left(v_{i}\left(p\delta_{ik}-\sigma_{ik}\right)+q_{k}\right)}{\partial x_{k}} & =0\\
\frac{\partial A_{ij}}{\partial t}+v_{k}\frac{\partial A_{ik}}{\partial x_{j}}+A_{ik}\frac{\partial v_{k}}{\partial x_{j}}+v_{k}\left(\frac{\partial A_{ij}}{\partial x_{k}}-\frac{\partial A_{ik}}{\partial x_{j}}\right) & =-\frac{\psi_{ij}}{\theta_{1}(\tau_{1})}\\
v_{i}\frac{\partial\rho}{\partial t}+\rho\frac{\partial v_{i}}{\partial t}+v_{i}\frac{\partial\left(\rho v_{k}\right)}{\partial x_{k}}+\rho v_{k}\frac{\partial v_{i}}{\partial x_{k}}+\frac{\partial\left(p\delta_{ik}-\sigma_{ik}\right)}{\partial x_{k}} & =0\\
J_{i}\frac{\partial\rho}{\partial t}+\rho\frac{\partial J_{i}}{\partial t}+J_{i}\frac{\partial\left(\rho v_{k}\right)}{\partial x_{k}}+\rho v_{k}\frac{\partial J_{i}}{\partial x_{k}}+\frac{\partial T}{\partial x_{i}} & =-\frac{\rho H_{i}}{\theta_{2}(\tau_{2})}
\end{align}

\end_inset


