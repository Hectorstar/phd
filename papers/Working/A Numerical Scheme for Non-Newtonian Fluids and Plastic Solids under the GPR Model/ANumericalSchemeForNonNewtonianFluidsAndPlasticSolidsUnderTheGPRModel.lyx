\backslash
begin{subequations}
\end_layout

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\begin{align}
v & =\frac{\rho}{k}\left(\frac{\Delta p}{K}\right)^{1/n}\left(\left(\frac{L}{2}\right)^{k}-\left(x-\frac{L}{2}\right)^{k}\right)\\
k & =\frac{n+1}{n}
\end{align}

\end_inset


\end_layout

\begin_layout Standard
\begin_inset ERT
status open

\begin_layout Plain Layout


\backslash
end{subequations}
\end_layout

\end_inset


\end_layout

