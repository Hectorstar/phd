\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\begin{align}
\left(\Pi_{k}\right)_{ij} & =\frac{\partial\sigma_{id}}{\partial A_{jk}}\\
\boldsymbol{w} & =T_{p}\frac{\partial\boldsymbol{\sigma_{d}}}{\partial\rho}+T_{\rho}\boldsymbol{e_{d}}\\
